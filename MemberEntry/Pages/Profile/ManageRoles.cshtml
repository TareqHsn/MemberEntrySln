@page
@model MemberEntry.Pages.Profile.ManageRolesModel
@{
}
<h2>Manage Roles</h2>
<table class="table">
    <thead>
        <tr>
            <th>Role Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model.Roles)
        {
            <tr>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>
<a asp-page="/Profile/CreateRole" class="btn btn-primary">Create New Role</a>